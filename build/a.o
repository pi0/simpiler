
## application main entry point
.text
main:

## Load variables
lw $t0,var_xptb
move $t1,$0

## add
add $t2,$t0,$t1
sw $t2,var_a

## While Start
WhileStart_xfxc:

## Load variables
lw $t0,var_xpec
move $t1,$0

## add
add $t2,$t0,$t1

## Check while condition
beqz $t2,WhileEnd_xfxc

## print string
li $v0,4
la $a0,var_xfse
syscall
j WhileStart_xfxc

## End of while
WhileEnd_xfxc:

## tell system to terminate application
exit:
li $v0,10
syscall

## end of application main entry point

## Allocate and label memory for variables
.data
var_xptb:	.word 5
var_xpec:	.word 1
var_xfse:	.asciiz "."
var_a:	.word 0

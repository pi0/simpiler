
## application main entry point
.text
main:

## While Start
WhileStart_xfnd:

## Load variables
lw $t0,var_a
lw $t1,var_xkve

## slt
slt $t2,$t0,$t1

## Check while condition
beqz $t2,WhileEnd_xfnd

## Load variables
lw $t0,var_xaff
move $t1,$0

## add
add $t2,$t0,$t1
sw $t2,var_i

## Load variables
lw $t0,var_xkvh
move $t1,$0

## add
add $t2,$t0,$t1
sw $t2,var_is_prime

## While Start
WhileStart_xpyh:

## Load variables
lw $t0,var_i
lw $t1,var_a

## slt
slt $t2,$t0,$t1

## Check while condition
beqz $t2,WhileEnd_xpyh

## Load variables
lw $t0,var_a
lw $t1,var_i

## rem
rem $t2,$t0,$t1
sw $t2,var_mod

## Load variables
lw $t0,var_mod
lw $t1,var_xfikb

## seq
seq $t2,$t0,$t1

## start of if
beqz $t2,IFElse_xaakb

## Load variables
lw $t0,var_xaumb
move $t1,$0

## add
add $t2,$t0,$t1
sw $t2,var_is_prime
j IFEnd_xaakb

## IF Else part
IFElse_xaakb:

## End of if
IFEnd_xaakb:

## Load variables
lw $t0,var_i
lw $t1,var_xucmb

## add
add $t2,$t0,$t1
sw $t2,var_i
j WhileStart_xpyh

## End of while
WhileEnd_xpyh:

## Load variables
lw $t0,var_is_prime
lw $t1,var_xauob

## seq
seq $t2,$t0,$t1

## start of if
beqz $t2,IFElse_xuhnb

## print integer
li $v0,1
lw $a0,var_a
syscall

## print string
li $v0,4
la $a0,var_xptqb
syscall
j IFEnd_xuhnb

## IF Else part
IFElse_xuhnb:

## End of if
IFEnd_xuhnb:

## Load variables
lw $t0,var_a
lw $t1,var_xfisb

## add
add $t2,$t0,$t1
sw $t2,var_a
j WhileStart_xfnd

## End of while
WhileEnd_xfnd:

## tell system to terminate application
exit:
li $v0,10
syscall

## end of application main entry point

## Allocate and label memory for variables
.data
var_xakd:	.word 2
var_xkve:	.word 50
var_xaff:	.word 2
var_xkvh:	.word 1
var_xfikb:	.word 0
var_xaumb:	.word 0
var_xucmb:	.word 1
var_xauob:	.word 1
var_xptqb:	.asciiz "\n"
var_xfisb:	.word 1
var_a:	.word 0
var_i:	.word 0
var_is_prime:	.word 0
var_mod:	.word 0
